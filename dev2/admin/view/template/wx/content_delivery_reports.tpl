function loadGraphs() {
    date_from = $('[name=date_from]').val();
    date_to = $('[name=date_to]').val();
    transferandhits_chart.showLoading("Loading...");
    transfer_chart.showLoading("Loading...");
    cachehits_chart.showLoading("Loading...");
    noncachehits_chart.showLoading("Loading...");
    filesizes_chart.showLoading("Loading...");
	
    var next_date = new Date(date_from.replace(/-/g, "/") + ' UTC');
	$.ajax({
		url: BASE_URL + '/public/cdn/reportstats.json',
		type: 'POST',
		dataType: 'JSON',
		data: 'key=<?php echo $this->config->get('wx_cdn_api_key'); ?>&token=<?php echo $this->config->get('wx_cdn_api_token'); ?>&grouping=' + (date_from==date_to? 'hourly' : 'daily') + '&date_from=' + date_from + '&date_to=' + date_to,
		success: function(json) {
			var allTransfers = [], allHits = [], allCacheHits = [];
			// split the data return into lines and parse them
			jQuery.each(json.stats, function(i, line) {
				date = Date.parse(line.timestamp.replace(/-/g, "/") + ' UTC');
				
				var d = new Date(line.timestamp.replace(/-/g, "/") + ' UTC');
				while (next_date < d) {
					var arr = [Date.parse(next_date.toUTCString()), 0];
					allTransfers.push(arr);
					allHits.push(arr);
					allCacheHits.push(arr);
					next_date.setDate(next_date.getDate() + 1);
				}

				allTransfers.push([
					date,
					parseInt(line.size, 10)
				]);
				allHits.push([
					date,
					parseInt(line.noncache_hit, 10)
				]);
				allCacheHits.push([
					date,
					parseInt(line.cache_hit, 10)
				]);
				next_date.setDate(next_date.getDate() + 1);
			});

			var d = new Date(date_to.replace(/-/g, "/") + ' UTC');
			while (next_date <= d) {
				var arr = [Date.parse(next_date.toUTCString()), 0];
				allTransfers.push(arr);
				allHits.push(arr);
				allCacheHits.push(arr);
				next_date.setDate(next_date.getDate() + 1);
			}

			$('#transferamount + div .summary').html(humanBytes(json.summary.size));
			$('#cachehits + div .summary').html(Highcharts.numberFormat(json.summary.cache_hit, 0) + " <span class='muted'>(" + Highcharts.numberFormat(json.summary.cache_hit/json.summary.hit*100, 2) + "%)</span>");
			$('#noncachehits + div .summary').html(Highcharts.numberFormat(json.summary.noncache_hit, 0) + " <span class='muted'>(" + Highcharts.numberFormat(json.summary.noncache_hit/json.summary.hit*100, 2) + "%)</span>");

			transferandhits_chart.series[0].setData(allTransfers);
			transferandhits_chart.series[1].setData(allCacheHits);
			transferandhits_chart.series[2].setData(allHits);
			transfer_chart.series[0].setData(allTransfers);
			cachehits_chart.series[0].setData(allCacheHits);
			noncachehits_chart.series[0].setData(allHits);

			transferandhits_chart.hideLoading();
			transfer_chart.hideLoading();
			cachehits_chart.hideLoading();
			noncachehits_chart.hideLoading();
		}
	});
	
	$.ajax({
		url: BASE_URL + '/public/cdn/reportfilesizes.json',
		type: 'POST',
		dataType: 'JSON',
