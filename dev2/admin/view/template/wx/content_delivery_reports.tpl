				["10K-50K", (parseInt(json.filesizes[0].le_50k_hit, 10)/summary*100)],
				["50K-100K", (parseInt(json.filesizes[0].le_100k_hit, 10)/summary*100)],
				["100K-500K", (parseInt(json.filesizes[0].le_500k_hit, 10)/summary*100)],
				["500K-1M", (parseInt(json.filesizes[0].le_1m_hit, 10)/summary*100)],
				["1M-10M", (parseInt(json.filesizes[0].le_10m_hit, 10)/summary*100)],
				["10M-100M", (parseInt(json.filesizes[0].le_100m_hit, 10)/summary*100)],
				["100M+", (parseInt(json.filesizes[0].gt_100m_hit, 10)/summary*100)]
			]);
			filesizes_chart.hideLoading();
		}
    });
}

