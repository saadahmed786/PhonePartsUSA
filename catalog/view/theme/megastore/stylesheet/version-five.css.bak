/* General */

body { background:#0b0808 url(../images/version-five/bg-body.png) top center repeat-y; }
.body-full-width, .body { background:#fff; }

/* Button */

.button { background:url(../images/version-five/bg-button.png) top left repeat-x; }

/* Top Bar */

div.top-bar { background:#262626; }

/* Top Bar -> Welcome Text */

div.top-bar .welcome-text a { color:#74be00; }

/* Header -> Shopping Cart */

div.header #cart .content table.cart tr .total { color:#74be00; }

/* Categories */

.categories, .categories-mobile-header { background:#82c600 url(../images/version-five/bg-categories.png) bottom left repeat-x; }

/* Box -> Products */

.box-product > div .price { color:#74be00; }
.product-list > div .price { color:#74be00; }
div.product-info .right .price .price-new { color:#74be00; }

/* Footer */

div#footer { background:#262626; }
.footer-top-outside h2, .footer-navigation h3 { color:#71b901; }

/* Twitter updates */

div#twitter-updates ul.tweet_list .tweet_text a { color:#74be00; }

/* Content -> Box Category */

.box-category ul li a:hover, .box-category ul li a.active { color:#74be00; }

/* Product Compare */

div.product-filter .product-compare a { color:#74be00; }